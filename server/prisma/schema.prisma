generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String       @id @default(uuid())
  email     String       @unique
  createdAt DateTime     @default(now())
  skills    UserSkill[]
}

model UserSkill {
  id        String   @id @default(uuid())
  userId    String
  skillName String
  rating    Int
  updatedAt DateTime @updatedAt

  user User @relation(fields: [userId], references: [id])

  @@unique([userId, skillName])
  @@index([userId])
  @@index([skillName])
}